{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": [
        {
          "Key": "Access-Control-Allow-Origin",
          "value": "https://mern-auth-frontend-alpha-two.vercel.app"
        },
      ] ,
      "dest": "server.js"
    }
  ]
} 
